{
  "name": "4-mans-project",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/4MansArquitectes/frontend.git"
  },
  "bugs": {
    "url": "https://github.com/4MansArquitectes/frontend/issues"
  },
  "keywords": [],
  "author": "Tutitoos, Dreapediez",
  "license": "ISC",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "prepare": "husky install",
    "lint:eslint": "eslint --ignore-path .gitignore . --max-warnings 0",
    "lint:staged": "lint-staged",
    "lint": "npm run lint:eslint && npm run lint:staged"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/index.tsx",
      "!src/setupTests.ts",
      "!src/react-app-env.d.ts",
      "!src/reportWebVitals.ts",
      "!src/styles/*"
    ],
    "transform": {
      ".+\\.[t|j]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!axios)/"
    ]
  },
  "dependencies": {
    "@fontsource/montserrat": "4.5.13",
    "@fontsource/roboto": "4.5.8",
    "@remix-run/router": "1.2.1",
    "@sentry/node": "7.30.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "axios": "1.2.2",
    "cypress": "12.3.0",
    "jest": "29.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.7.1",
    "react-router-dom": "6.6.2",
    "react-scripts": "5.0.1",
    "react-test-renderer": "18.2.0",
    "styled-components": "5.3.6",
    "typescript": "4.9.4",
    "web-vitals": "3.1.1"
  },
  "devDependencies": {
    "@types/estree": "1.0.0",
    "@types/jest": "29.2.5",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@types/react-test-renderer": "18.0.0",
    "@types/styled-components": "5.1.26",
    "husky": "8.0.0",
    "lint-staged": "13.1.0",
    "msw": "0.49.2"
  }
}
